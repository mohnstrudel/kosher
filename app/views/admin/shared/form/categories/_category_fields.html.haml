-if params[:sublevel]
  -if object.respond_to?(:parent_id)
    .form-group
      %label.col-lg-2.control-label
        =t('admin.form.model.parent_id')
      .col-lg-10
        / =f.collection_select(:parent_id, object.class.name.constantize.all, :id, :title, {include_blank: true, :include_hidden => true, :selected => nil}, { class: 'form-control m-b'})
        =f.select :parent_id, object.class.name.constantize.all.map { |obj| [ obj.title, obj.id ] }, {}, { class: "form-control m-b" }
    .hr-line-dashed

